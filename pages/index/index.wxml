<view class="topWrap">
    <view  class="inner">

        <view class='avatar'></view>

        <view class="content t_center">
            <view><image src="/images/homePage/name.png"/></view>
            <view class='f28 bold'>5年+工作经验</view>
            <view class='f24'>“正确的做设计”比“做正确的设计重要的多”</view>
        </view>
    </view>


</view>

<view class="contentWrap">
  <!-- 问与答 -->
  <view class="titleWrap">
    <view>
      <view class='bold f28'>问与答</view>
      <view class='f22'>Q&A</view>
    </view>

    <view class='f24 g9b right'>已解答10个问题</view>
  </view>

  <view class="content">
    <view class="QAList f40">
      <view wx:for="{{QAList}}" class="{{item.drop?'dropAnimation':item.className}}" catchtap='tap' data-index='{{index}}' data-item='{{item}}' wx:key="index">{{item.Q}}</view>
    </view>
  </view>

  <view class="nav f28 grey9">
    <view wx:for="{{nav}}"
          wx:key="{{index}}"
          class="{{navIndex === index?'active':''}}"
          data-index='{{index}}'
          bindtap='changeNav'>{{item.name}}</view>
  </view>

</view>


<view class="navContent">
  <block wx:for="{{proList}}" wx:key="{{index}}">
    <view>
      <image src='https://6465-dev-d6b769-1258442598.tcb.qcloud.la/images/programs/pro_01.png?sign=d9dc2e7a36bfb42b8c4ee03bf0856895&t=1552488323'></image>
      <view class="f34 bold title">{{item.name}}</view>
      <view class='f24 grey9 introduce'>{{item.introduce}}</view>
      <view class='f24 time'>{{item.time}}</view>
    </view>

  </block>
</view>

